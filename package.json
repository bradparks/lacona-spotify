{
  "name": "lacona-spotify",
  "version": "0.1.0",
  "description": "Play music in the Spotify app",
  "main": "lib/index.js",
  "lacona": {
    "title": "Spotify",
    "description": "Play music in the Spotify app",
    "config": "config.json"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "lacona",
    "lacona-addon"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandonhorst/lacona-spotify.git"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "rimraf": "^2.5.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  },
  "bugs": {
    "url": "https://github.com/brandonhorst/lacona-spotify/issues"
  },
  "homepage": "https://github.com/brandonhorst/lacona-spotify#readme",
  "dependencies": {
    "elliptical": "^1.0.0-rc15",
    "lacona-api": "^0.5.1",
    "lacona-phrases": "^0.7.0"
  },
  "author": "Brandon Horst <brandonhorst@gmail.com> (https://github.com/brandonhorst)"
}
